package main

templ Header(email string, success bool) {
	@scopedcss_Header.Once() {
		<style>.buttons.jsx-1ERt5d { display: flex; align-items: center; position: absolute; right: 10px; top: 20px }
.buttons.jsx-1ERt5d > * { margin-right: 15px }
.buttons.jsx-1ERt5d > *:last-child { margin-right: 0px }
</style>
	}
	<header class="jsx-1ERt5d">
		<nav class="jsx-1ERt5d buttons">
			@Button("mailto:" + email) {
				Contact
			}
			@Button("/faq") {
				FAQ
			}
			@SlackButton(success)
		</nav>
	</header>
}

//Generated by scopedcss
var scopedcss_Header = templ.NewOnceHandle()
